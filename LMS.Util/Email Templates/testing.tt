<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>

<#@ parameter name="Name" type="System.String" #>
<#@ parameter name="Designation" type="System.String" #>
<#@ parameter name="ContactNumber" type="System.String" #>
<#@ parameter name="Type" type="System.String" #>
<#@ parameter name="Unit" type="System.String" #>
<#@ parameter name="StartDate" type="System.String" #>
<#@ parameter name="EndDate" type="System.String" #>
<#@ parameter name="CreatedOn" type="System.String" #>

<!DOCTYPE> 
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
</head>
<body>
    LEAVE APPLIED
    <table>
        <tr>
            <td><b>Name :</b></td>
            <td><#= Name #></td>
        </tr>
        <tr>
            <td><b>Designation :</b></td>
            <td><#= Designation #></td>
        </tr>
        <tr>
            <td><b>Contact Number :</b></td>
            <td><#= ContactNumber #></td>
        </tr>
		<tr>
            <td><b>Type :</b></td>
            <td><#= Type #></td>
        </tr>
		<tr>
            <td><b>Unit :</b></td>
            <td><#= Unit #></td>
        </tr>
        <tr>
            <td><b>From :</b></td>
            <td><#= StartDate #></td>
        </tr>
        <tr>
            <td><b>To :</b></td>
            <td><#= EndDate #></td>
        </tr>
        <tr>
            <td><b>Applied On :</b></td>
            <td><#= CreatedOn #></td>
        </tr>
    </table>


</body>
</html>